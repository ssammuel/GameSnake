<Application x:Class="GameSnake.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GameSnake"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#FF191970</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgroundColor">#FF000080</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#FF0000FF</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#FFF5F5F5</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#FF469284</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Oriod Sana Momo</FontFamily>
    </Application.Resources>
</Application>
